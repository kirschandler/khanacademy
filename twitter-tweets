SELECT * FROM twitter;

ALTER TABLE twitter ADD emoji TEXT;

SELECT * FROM twitter;

UPDATE twitter SET emoji = "hungry" WHERE id = 1;
UPDATE twitter SET emoji = "money bag" WHERE id = 2;
UPDATE twitter SET emoji = "heart eyes" WHERE id = 3;
UPDATE twitter SET emoji = "inspired" WHERE id = 4;
UPDATE twitter SET emoji = "drool face" WHERE id = 5;

SELECT * FROM twitter;

